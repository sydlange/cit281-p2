// Returns a random number between min (inclusive) and max (exclusive)
function getRandomInteger(min, max) {
    return Math.floor(Math.random() * (20 - 10) + 10);
}

const alphabet = "abcdefghijklmnopqrstuvwxyz".split("");
let result = "";

for (let i = 0; i < getRandomInteger(5, 26); i++) {
    result += getRandomLetter(alphabet[getRandomInteger(1,alphabet.length-1)]);
}

console.log(getRandomString(result));

// Returns random lowercase letter
function getRandomLetter() {
    return alphabet[Math.floor(Math.random() * alphabet.length)]
  }

//Returns random string length
function getRandomString(minLength, maxLength){
    return getRandomLetter(Math.floor(Math.random() * (minLength - maxLength) + maxLength));
  }
  for (let i = 0; i < getRandomString(10, 20); i++){
      result +=alphabet[getRandomString(1,alphabet.length-1)];
  }
// Returns string in ascending order
function getSortedString(string){
    char ;tempArray[string] = inputString.toCharArray();
	Arrays.sort(tempArray);
		return new String(tempArray);
	}
  
